<p id="notice"><%= notice %></p>
<% if student_signed_in? %>
<p>
  <strong>Student:</strong>
  <%= @student_application.student_id %>
</p>

<p>
  <strong>Name:</strong>
  <%= @student_application.name %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @student_application.phone %>
</p>

<p>
  <strong>Email:</strong>
  <%= @student_application.email_id %>
</p>

<p>
  <strong>GPA:</strong>
  <%= @student_application.gpa %>
</p>

<%= link_to 'Edit', edit_student_application_path(@student_application) %> |
<!-- <%= link_to 'Back', student_application_path %> | -->
<%= link_to 'Destroy', @student_application, method: :delete, data: { confirm: 'Are you sure?' } %> |

<%= link_to 'Sign Out', destroy_student_session_path, :method => :delete %>
<% else %>
  <li><%= link_to '<button type="button">Student Login</button>'.html_safe, new_student_session_path %></li>
<% end %>
